            }
          }
        },
        "team-permissions": {
          "type": "array",
          "items": {
            "type": "object",
            "properties": {
              "teamString": { "type": "string" },
              "UUID": { "type": "string" },
              "permission": { "type": "string" }
            }
          }
        },
        "license": { "type": "string" },
        "name": { "type": "string" }
      }
