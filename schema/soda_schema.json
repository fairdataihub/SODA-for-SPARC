    "digital-metadata": {
      "type": "object",
      "properties": {
        "description": { "type": "object" },
        "pi-owner": {
          "type": "object",
          "properties": {
            "name": { "type": "string" },
            "userString": { "type": "string" },
            "UUID": { "type": "string" }
          }
