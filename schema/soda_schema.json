    "dataset-entity-obj": {
      "type": "object",
      "properties": {
        "bucketed-data": {
          "type": "object",
          "properties": {
            "code": {
              "type": "array",
              "items": { "type": "string", "format": "file-path" }
            },
            "experimental-data": {
              "type": "array",
              "items": { "type": "string", "format": "file-path" }
